{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle" :0,
        "linebreak-style" :0,
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always",
              "jsx": "never",
              "ts": "never",
              "tsx": "never"
            }
          ],
          "prettier/prettier": [
            2,
            {
              "printWidth": 120,
              "singleQuote": true,
              "trailingComma": "none",
              "tabWidth": 2,
              "endOfLine": "auto"
            }
          ]
    }
}
